#include<stdio.h>
void main()
{
  int a,b,c;
  float d,e;
  printf("The value of A for addition:\n",a);
  scanf("%d",&a);
  printf("The value of B for addition:\n",b);
  scanf("%d",&b);
  c=a+b;
  printf("Addition:%d \n",c);
  printf("The value of A for Subtraction:\n",a);
  scanf("%d",&a);
  printf("The value of B for subtraction:\n",b);
  scanf("%d",&b);
  c=a-b;
  printf("Subtraction:%d \n",c);
  printf("The vlaue of A for multiply:\n ",a);
  scanf("%d",&a);
  printf("The value of B for multiply:\n",b);
  scanf("%d",&b);
  c=a*b;
  printf("Multiply:%d \n",c);
  printf("The value of A for division:\n",a);
  scanf("%d",&a);
  printf("The vlaue of B for division:\n",b);
  scanf("%d",&b);
  c=a/b;
  printf("Division:%d \n",c);
  printf("The value of A for modulation:\n",a);
  scanf("%d",&a);
  printf("The value of B for modulation:\n",b);
  scanf("%d",&b);
  c=a%b;
  printf("modulation:%d \n",c);
  getch();
}
